.about-main{
  min-height: 1206rpx;
  display: flex;
  margin: 200rpx 0 0 0;
  flex-direction: column;
  text-align: center;
  font-size: 32rpx;
  padding: 0 25rpx;
}
.content{
  color: #9c9c9c;
  font-size: 36rpx;
}
.logo-img{
  width: 200rpx;
  height: 200rpx;
  margin: 0 auto;
}
.about-phone-box{
  position: absolute;
  bottom: 60rpx;
  display: block;
  left: calc(50% - 248rpx);
}
.phone-text{
  border-bottom:1rpx solid #538afe;
}