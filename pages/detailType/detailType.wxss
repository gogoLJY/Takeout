.detail-main{
  width: 750rpx;
  display: flex;
  flex-direction: column;
  color: #333;
  font-family: PingFang-SC-Medium;
  font-size: 28rpx;
  margin-bottom: 60rpx;
}
.detail-top{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  width: calc(100% - 25px);
	background-color: #ffffff;
	border-bottom: solid 1px rgba(177,184,191,.4);
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 33rpx 25rpx;
}
.detail-type-name{
	font-size: 36rpx;
	color: #999999;
}
.finish-text{
  font-size: 32rpx;
  border-left: 1rpx solid rgba(177,184,191,.4);
  padding-left: 20rpx;
}
.list-type{
  text-align: center;
}
.detail-list-box{
  margin-top: 60rpx;
}
.detail-list{
  float: left;
  margin-top: 40rpx;
  padding: 0 0 0 40rpx;
}
.detail-item{
  display: inline-block;
  box-sizing:border-box;
  width: 70rpx;
  text-align: center;
  margin: 0 40rpx 0 0; 
}
.type-img{
  width: 74rpx;
	height: 74rpx;
}
.type-name{
  display: block;
  margin-top: 10rpx;
}